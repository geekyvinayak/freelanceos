# ===========================================
# FreelanceOS Environment Configuration
# ===========================================

# Supabase Configuration
# Get these from your Supabase project dashboard
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Gemini API Configuration
# Get your API key from Google AI Studio
VITE_GEMINI_API_KEY=your_gemini_api_key

# Application Configuration
VITE_APP_NAME=FreelanceOS
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional freelancer management platform

# Environment Configuration
NODE_ENV=production
VITE_APP_ENV=production

# Database Reset Configuration (Demo Environment)
VITE_RESET_ENABLED=true
VITE_RESET_INTERVAL=daily
VITE_RESET_NOTIFY_USERS=true

# Vercel Cron Configuration (Backend Only)
CRON_SECRET=your_secure_random_string_for_cron_auth
ADMIN_API_KEY=your_admin_api_key_for_manual_triggers
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Production Database Configuration
# Use separate Supabase project for production
SUPABASE_URL=https://your-production-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# Security Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_BUNDLE_ANALYZER=false

# Feature Flags
VITE_ENABLE_DEMO_MODE=false
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_PASSWORD_RESET=true

# Analytics Configuration (Optional)
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Error Reporting Configuration (Optional)
VITE_SENTRY_DSN=your_sentry_dsn

# Email Configuration (Optional - for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Custom Domain Configuration (Optional)
VITE_CUSTOM_DOMAIN=https://your-domain.com
